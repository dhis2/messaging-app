{
  "name": "messaging-app",
  "version": "31.0.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "prestart": "npm run localize",
    "pretest": "npm run localize",
    "prebuild": "npm run localize",
    "lint:style": "d2-style js check --all",
    "extract-pot": "d2-i18n-extract -p src/ -o i18n/",
    "localize": "npm run extract-pot && d2-i18n-generate -n messaging-app -p ./i18n/ -o ./src/locales/"
  },
  "dependencies": {
    "@dhis2/app-runtime": "^2.0.3",
    "@dhis2/d2-i18n": "^1.0.6",
    "@dhis2/ui": "^1.0.0-beta.14",
    "@dhis2/ui-core": "^4.1.1",
    "@dhis2/ui-widgets": "^2.0.5",
    "d2": "31.0.2",
    "d2-ui": "^29.0.25",
    "d2-utilizr": "^0.2.15",
    "history": "^4.7.2",
    "loglevel": "^1.6.1",
    "material-ui": "^0.20.0",
    "material-ui-chip-input": "^0.19.0",
    "material-ui-icons": "1.0.0-beta.36",
    "moment": "^2.21.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-linkify": "^0.2.2",
    "react-redux": "^7.0.3",
    "react-router-dom": "^5.0.0",
    "react-scripts": "3.0.1",
    "recompose": "^0.30.0",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "typeface-roboto": "^0.0.75"
  },
  "devDependencies": {
    "@dhis2/cli-style": "^3.2.2",
    "@dhis2/d2-i18n-extract": "^1.0.8",
    "@dhis2/d2-i18n-generate": "^1.1.1",
    "husky": "^2.3.0",
    "redux-logger": "3.0.6"
  },
  "husky": {
    "hooks": {
      "commit-msg": "d2-style commit check",
      "pre-commit": "d2-style js apply"
    }
  }
}
