{
    "presets": [["es2015", { "modules": false }], "react", "stage-2"],
    "env": {
        "production": {
            "presets": ["react-optimize"]
        },
        "test": {
            "presets": [["es2015"], "react"]
        }
    },
    "plugins": [
        [
            "transform-imports",
            {
                "rxjs": {
                    "transform": "rxjs/operator/${member}",
                    "preventFullImport": true
                }
            }
        ]
    ]
}
